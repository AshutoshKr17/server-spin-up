{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/your-username/server-spin-up"
    },
    {
      "pattern": "^https://img.shields.io"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/your-username/server-spin-up/blob/main/"
    }
  ],
  "httpStatusCodes": {
    "400": "warn",
    "401": "warn",
    "403": "warn",
    "404": "error",
    "405": "warn",
    "500": "error",
    "502": "warn"
  },
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttp2": false
}
